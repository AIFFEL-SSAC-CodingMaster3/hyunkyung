   "source": [
    "def arrayPairSum():\n",
    "    pairsum = 0\n",
    "    nums.sort()\n",
    "    \n",
    "    for i in range(len(nums)):\n",
    "        if i % 2 == 0:\n",
    "            pairsum += nums[i]\n",
    "    return pairsum"
   ]
